ul.list-group
    li.list-group-item(ng-repeat='shopkin in shopkins')
        .row
          .col-md-12
            h4
              | {{ shopkin.name }}
            |       
            h5
              | Season Number {{ shopkin.season }}
        |   
        .row
          .col-md-3
            div(ng-if='shopkin.processedImageUrl')
              img(ng-src='/processed/{{shopkin.processedImageUrl}}')
          |     
          .col-md-3
            | {{ shopkin.number }}
          |     
          .col-md-2.col-md-offset-4
            ul
              li(ng-repeat="person in shopkin.ownedBy")
                {{ person.username }}
            {{ shopkin.ownedBy.username }}
            span.label(style='display:block', ng-class='{"label-default": shopkin.value=="common", "label-warning": shopkin.value=="ultra_rare", "label-success": shopkin.value=="rare"}')
              h5 {{ shopkin.value }}
